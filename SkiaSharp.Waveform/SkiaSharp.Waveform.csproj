<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.2.0</Version>
    <Authors>Tom Alabaster</Authors>
    <Description>A library for drawing and generating waveforms using SkiaSharp.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>2018 Tom Alabaster</Copyright>
    <RepositoryUrl>https://github.com/tomalabaster/SkiaSharp.Waveform</RepositoryUrl>
    <PackageProjectUrl>https://github.com/tomalabaster/SkiaSharp.Waveform</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/tomalabaster/SkiaSharp.Waveform/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SkiaSharp" Version="1.60.3" />
  </ItemGroup>

</Project>
